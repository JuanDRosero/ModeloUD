@model Sede
@{
    ViewData["Title"] = "Editar Sede";
}
<div class="card-body">
    <form method="post" asp-action="Edit">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-floating mb-3">
                <input class="form-control" id="inputIdentificador" asp-for="Id" placeholder="Identificador" maxlength="3" />
                <label asp-for="Id">Identificador de la Sede</label>
                <span asp-validation-for="Id" class="text-danger"></span>
            </div>
            <div class="form-floating mb-3">
                <input class="form-control" id="inputSede" asp-for="Nombre" placeholder="Nombre de la Sede" />
                <label asp-for="Nombre">Nombre de la Sede</label>
                <span asp-validation-for="Nombre" class="text-danger"></span>
            </div>
             <div class="mt-4 mb-0">
                <div class="d-grid"><button class="btn btn-primary btn-block" type="submit">Guardar Sede</button></div>
             </div>
    </form>
</div>